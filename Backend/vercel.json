{
    "version": 2,
    "builds": [
      {
        "src": "Frontend/**",
        "use": "@vercel/static"
      },
      {
        "src": "Backend/run.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/Frontend/$1"
      },
      {
        "src": "/api/(.*)",
        "dest": "/Backend/run.py"
      }
    ]
  }
  
  